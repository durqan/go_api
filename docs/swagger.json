    "definitions": {
        "address.CreateAddressRequest": {
            "type": "object",
            "required": [
                "City",
                "Full",
                "House",
                "Region",
                "Street"
            ],
            "properties": {
                "City": {
                    "type": "string"
                },
                "Full": {
                    "type": "string"
                },
                "House": {
                    "type": "string"
                },
                "Region": {
                    "type": "string"
                },
                "Room": {
                    "type": "string"
                },
                "Street": {
                    "type": "string"
                }
            }
        },
        "passport.CreatePassportRequest": {
            "type": "object",
            "required": [
                "IssueDate",
                "IssueDepartment",
                "Number",
                "Series"
            ],
            "properties": {
                "BirthPlace": {
                    "type": "string",
                    "maxLength": 200
                },
                "IssueDate": {
                    "type": "string"
                },
                "IssueDepartment": {
                    "type": "string",
                    "maxLength": 500,
                    "minLength": 5
                },
                "Number": {
                    "type": "string"
                },
                "Series": {
                    "type": "string"
                }
            }
        },
        "user.CreateUserRequest": {
            "type": "object",
            "required": [
                "BirthDate",
                "Email",
                "Firstname",
                "Lastname",
                "Phone",
                "Sex"
            ],
            "properties": {
                "BirthDate": {
                    "type": "string"
                },
                "Email": {
                    "type": "string"
                },
                "Firstname": {
                    "type": "string",
                    "maxLength": 100,
                    "minLength": 1
                },
                "Lastname": {
                    "type": "string",
                    "maxLength": 100,
                    "minLength": 1
                },
                "Patronymic": {
                    "type": "string",
                    "maxLength": 100
                },
                "Phone": {
                    "type": "string"
                },
                "Sex": {
                    "type": "string",
                    "enum": [
                        "male",
                        "female"
                    ]
                }
            }
        }
    }
}